  "workbench.colorCustomizations": {
    "editor.background": "#181818db",
    "C_Cpp.enhancedColorization": "Enabled"
  },
  "editor.wordWrap": "on",
  "editor.semanticTokenColorCustomizations": {
    "rules": {
      "type:cpp": {
        "foreground": "#2bdfd6"
      },
      "variable:cpp": {
        "foreground": "#99D7F8"
      },
      "property:cpp": {
        "foreground": "#99D7F8"
      },
      "namespace:cpp": {
        "foreground": "#2bdfd6",
        "fontStyle": "italic bold underline"
      }
    }
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "source.note comment",
        "settings": {
          "foreground": "#88846f"
        }
      },
      {
        "scope": "source.note string",
        "settings": {
          "foreground": "#66D9EF"
        }
      },
      {
        "scope": "source.note entity.name.function",
        "settings": {
          "foreground": "#A6E22E"
        }
      },
      {
        "scope": "source.note variable.parameter",
        "settings": {
          "foreground": "#FD971F",
          "fontStyle": ""
        }
      },
      {
        "scope": "source.note support.function",
        "settings": {
          "foreground": "#E6DB74"
        }
      },
      {
        "scope": "source.note support.constant",
        "settings": {
          "foreground": "#E6DB74"
        }
      },
      {
        "scope": "source.note keyword",
        "settings": {
          "foreground": "#F92672"
        }
      },
      {
        "scope": "source.note invalid",
        "settings": {
          "foreground": "#F92672",
          "fontStyle": "bold underline"
        }
      }
    ]
  },
